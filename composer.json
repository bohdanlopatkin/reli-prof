{
  "name": "sj-i/php-profiler",
  "description": "a profiler for php",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "sji",
      "homepage": "https://twitter.com/sji_ch"
    }
  ],
  "config": {
    "platform": {
      "php": "7.4.0"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^3.11",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "PhpProfiler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpProfiler\\": "tests"
    }
  },
  "require": {
    "ext-ffi": "*",
    "ext-pcntl": "*",
    "symfony/console": "^5.0"
  }
}
